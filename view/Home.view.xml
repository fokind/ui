<mvc:View
  xmlns:mvc="sap.ui.core.mvc"
  xmlns:uxap="sap.uxap"
  xmlns:layout="sap.ui.layout"
  xmlns="sap.m"
  xmlns:customdata="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
  controllerName="tms.basic.controller.Home"
  height="100%">
  <Page id="home" title="{i18n>title}">
    <content>
      <uxap:ObjectPageLayout>
        <uxap:sections>
          <uxap:ObjectPageSection>
            <uxap:subSections>
              <uxap:ObjectPageSubSection title="Основное">
                <uxap:blocks>
                  <layout:BlockLayout>
                    <layout:content>
                      <layout:BlockLayoutRow>
                        <layout:content>
                          <layout:BlockLayoutCell>
                            <layout:content>
                              <GenericTile class="sapUiTinyMargin tileLayout" header="{i18n>homeLogin}" press="onPress" customdata:target="login"/>
                              <GenericTile class="sapUiTinyMargin tileLayout" header="{i18n>homeItems}" press="onPress" customdata:target="items"/>
                              <GenericTile class="sapUiTinyMargin tileLayout"/>
                              <GenericTile class="sapUiTinyMargin tileLayout"/>
                              <GenericTile class="sapUiTinyMargin tileLayout"/>
                              <GenericTile class="sapUiTinyMargin tileLayout"/>
                              <GenericTile class="sapUiTinyMargin tileLayout"/>
                              <GenericTile class="sapUiTinyMargin tileLayout"/>
                              <GenericTile class="sapUiTinyMargin tileLayout"/>
                            </layout:content>
                          </layout:BlockLayoutCell>
                        </layout:content>
                      </layout:BlockLayoutRow>
                    </layout:content>
                  </layout:BlockLayout>
                </uxap:blocks>
              </uxap:ObjectPageSubSection>
            </uxap:subSections>
          </uxap:ObjectPageSection>
        </uxap:sections>
      </uxap:ObjectPageLayout>
    </content>
  </Page>
</mvc:View>